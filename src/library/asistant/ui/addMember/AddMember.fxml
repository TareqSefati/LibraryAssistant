<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" fx:id="rootPane" prefHeight="241.0" prefWidth="515.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="library.asistant.ui.addMember.AddMemberController">
    <stylesheets>
        <URL value="@addmember.css" />
    </stylesheets>
   <children>
      <VBox alignment="CENTER" layoutX="10.0" layoutY="10.0" prefHeight="260.0" prefWidth="505.0">
         <children>
            <JFXTextField fx:id="name" labelFloat="true" promptText="Name">
               <VBox.margin>
                  <Insets left="10.0" right="10.0" top="20.0" />
               </VBox.margin>
            </JFXTextField>
            <JFXTextField fx:id="memberID" labelFloat="true" layoutX="20.0" layoutY="30.0" promptText="Member ID">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="20.0" />
               </VBox.margin>
            </JFXTextField>
            <JFXTextField fx:id="contact" labelFloat="true" layoutX="20.0" layoutY="30.0" promptText="Contact">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
            </JFXTextField>
            <JFXTextField fx:id="email" labelFloat="true" layoutX="10.0" layoutY="55.0" promptText="E-mail">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
            </JFXTextField>
            <HBox alignment="CENTER" maxWidth="480.0" nodeOrientation="LEFT_TO_RIGHT" prefHeight="43.0" prefWidth="480.0">
               <children>
                  <JFXButton fx:id="saveBtn" onAction="#addMember" prefHeight="60.0" prefWidth="240.0" stylesheets="@../addBook/addbook.css" text="Save">
                     <HBox.margin>
                        <Insets right="10.0" />
                     </HBox.margin></JFXButton>
                  <JFXButton fx:id="cancelBtn" layoutX="10.0" layoutY="10.0" onAction="#cancelMember" prefHeight="60.0" prefWidth="240.0" stylesheets="@../addBook/addbook.css" text="Cancel">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin></JFXButton>
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
